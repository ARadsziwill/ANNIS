          http://www.springframework.org/schema/util/spring-util-2.5.xsd http://www.springframework.org/schema/util/spring-util-3.0.xsd/spring-spring-util-3.0.xsd-3.0.xsd
">
  
	<!-- Enable bean autowiring -->
	<context:annotation-config/>
   
  	<!-- AnnisDao -->
