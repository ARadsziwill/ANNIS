<?xml version="1.0" encoding="UTF-8"?>

<!-- False positive Filter for FindBugs -->

<FindBugsFilter>
  
  <!-- automatically generated or external code that we can't influence -->
  <Match>
    <Or>
      <Class name="annis.visualizers.htmlvis.HTMLVisConfigLexer"/>
      <Class name="annis.visualizers.htmlvis.HTMLVisConfigParser"/>
    </Or>
  </Match>
  
  <!-- We deprecated the old Join class in order to avoid confusion and we possible need the backwards compatibility -->
  <Match>
    <And>
      <Bug pattenr="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS" />
      <Class name="annis.sqlgen.model.Join"/>
    </And>
  </Match>
  
  <Match>
    <Bug category="EXPERIMENTAL" />
  </Match>
  <Match>
    <Bug pattern="SE_NO_SERIALVERSIONID" />
  </Match>
</FindBugsFilter>
