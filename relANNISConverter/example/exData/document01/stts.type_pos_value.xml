<!DOCTYPE paula SYSTEM "paula_feat.dtd">
<paula version="1.0">

	<header paula_id="stts_value_pos" />


	<featList xmlns:xlink="http://www.w3.org/1999/xlink" type="value_pos"
		xml:base="stts.type_pos.xml">

		<!-- supertypes -->
		<feat xlink:href="#pos" value="pos" description="base type: pos" />
		<feat xlink:href="#openclass" value="openclass" description="open word classes" />
		<feat xlink:href="#closedclass" value="closedclass" description="closed word classes" />
		<feat xlink:href="#N" value="N" description="noun" />
		<feat xlink:href="#V" value="V" description="verb" />
		<feat xlink:href="#finite" value="finite" description="finite verbform" />
		<feat xlink:href="#nonfinite" value="nonfinite" description="non-finite verbform" />
		<feat xlink:href="#infinitive" value="infinitive" description="infinitive verbform" />
		<feat xlink:href="#participle" value="participle" description="past participle" />
		<feat xlink:href="#participle" value="participle" description="past participle" />
		<feat xlink:href="#imperative" value="imperative" description="imperative" />
		<feat xlink:href="#ADJ" value="ADJ" description="adjective" />
		<feat xlink:href="#P" value="P" description="proform" />
		<feat xlink:href="#prodemon" value="prodemon" description="demonstrative pronoun" />
		<feat xlink:href="#proindef" value="proindef" description="indefinite pronoun" />
		<feat xlink:href="#propos" value="propos" description="posessive pronoun" />
		<feat xlink:href="#prointer" value="prointer" description="interrogative pronoun" />
		<feat xlink:href="#KO" value="KO" description="conjunction" />
		<feat xlink:href="#KOU" value="KOU" description="subordinating conjunction" />
		<feat xlink:href="#AP" value="AP" description="adposition" />
		<feat xlink:href="#PT" value="PT" description="particle" />
		<feat xlink:href="#punctuation" value="punctuation" description="punctuation mark" />
		<feat xlink:href="#misc" value="misc" />

		<!-- base types -->
		<feat xlink:href="#ADV" value="ADV" description="adverb"
			example="schon, bald, doch" />
		<feat xlink:href="#ART" value="ART" description="definite or indefinite article"
			example="der, die, das, ein, eine" />
		<feat xlink:href="#CARD" value="CARD" description="cardinal number"
			example="zwei [Maenner], [im Jahre] 1994" />
		<feat xlink:href="#ITJ" value="ITJ" description="interjection"
			example="mhm, ach, tja" />
		<feat xlink:href="#NN" value="NN" description="common noun"
			example="Tisch, Herr, [das] Reisen" />
		<feat xlink:href="#NE" value="NE" description="proper noun"
			example="Hans, Hamburg, HSV" />
		<feat xlink:href="#VVFIN" value="VVFIN" description="finite full verb"
			example="du] gehst, [wir] kommen [an]" />
		<feat xlink:href="#VAFIN" value="VAFIN" description="finite auxiliary verb"
			example="[du] bist, [wir] werden" />
		<feat xlink:href="#VMFIN" value="VMFIN" description="finite modal verb"
			example="duerfen" />
		<feat xlink:href="#VVIZU" value="VVIZU" description="infinitive with zu, full verb"
			example="anzukommen, loszulassen" />
		<feat xlink:href="#VVINF" value="VVINF" description="inifinitive, full verb"
			example="gehen, ankommen" />
		<feat xlink:href="#VAINF" value="VAINF" description="infinitive, auxiliary verb"
			example="werden, sein" />
		<feat xlink:href="#VMINF" value="VMINF" description="infinitive, modal verb"
			example="wollen" />
		<feat xlink:href="#VVPP" value="VVPP" description="past participle, full verb"
			example="gegangen, angekommen" />
		<feat xlink:href="#VAPP" value="VAPP" description="past participle, auxiliary verb"
			example="gewesen" />
		<feat xlink:href="#VMPP" value="VMPP" description="past participle, modal verb"
			example="gekonnt, [er hat gehen] koennen" />
		<feat xlink:href="#VVPP" value="VVPP" description="past participle, full verb"
			example="gegangen, angekommen" />
		<feat xlink:href="#VAPP" value="VAPP" description="past participle, auxiliary verb"
			example="gewesen" />
		<feat xlink:href="#VMPP" value="VMPP" description="past participle, modal verb"
			example="gekonnt, [er hat gehen] koennen" />
		<feat xlink:href="#VVIMP" value="VVIMP" description="imperative, full verb"
			example="komm [!]" />
		<feat xlink:href="#VAIMP" value="VAIMP" description="imperative, auxiliary verb"
			example="sei [ruhig !]" />
		<feat xlink:href="#ADJA" value="ADJA" description="attributive adjective"
			example="[das] große [Haus]" />
		<feat xlink:href="#ADJD" value="ADJD"
			description="adverbal or predicative adjective" example="[er faehrt] schnell, [er ist] schnell" />
		<feat xlink:href="#PPER" value="PPER" description="irreflexive personal pronoun"
			example="ich, er, ihm, mich, dir" />
		<feat xlink:href="#PRF" value="PRF" description="reflexive pronoun"
			example="sich, einander, dich, mir" />
		<feat xlink:href="#PROAV" value="PROAV"
			description="pronominal adverb (tag used in TIGER corpus)" example="dafuer, dabei, deswegen, trotzdem" />
		<feat xlink:href="#PAV" value="PAV"
			description="pronominal adverb (tag used in original STTS)" example="dafuer, dabei, deswegen, trotzdem" />
		<feat xlink:href="#PDS" value="PDS"
			description="substitutive demonstrative pronoun" example="dieser, jener" />
		<feat xlink:href="#PDAT" value="PDAT"
			description="attributive demonstrative pronoun" example="jener [Mensch]" />
		<feat xlink:href="#PIS" value="PIS" description="substitutive indefinite pronoun"
			example="keiner, viele, man, niemand" />
		<feat xlink:href="#PIAT" value="PIAT" description="attributive indefinite pronoun"
			example="kein [Mensch], irgendein [Glas]" />
		<feat xlink:href="#PPOSS" value="PPOSS" description="substitutive possessive pronoun"
			example="meins, deiner" />
		<feat xlink:href="#PPOSAT" value="PPOSAT" description="attributive possessive pronoun"
			example="mein [Buch], deine [Mutter]" />
		<feat xlink:href="#PRELS" value="PRELS" description="substitutive relative pronoun"
			example="[der Hund,] der" />
		<feat xlink:href="#PRELAT" value="PRELAT" description="attributive relative pronoun"
			example="[der Mann ,] dessen [Hund]" />
		<feat xlink:href="#PWS" value="PWS"
			description="substitutive interrogative pronoun" example="wer, was" />
		<feat xlink:href="#PWAT" value="PWAT"
			description="attributive interrogative pronoun" example="welche [Farbe], wessen [Hut]" />
		<feat xlink:href="#PWAV" value="PWAV"
			description="interrogative adverb or adverbial relative pronoun"
			example="warum, wo, wann, worueber, wobei" />
		<feat xlink:href="#KON" value="KON" description="coordinating conjunction"
			example="und, oder, aber" />
		<feat xlink:href="#KOKOM" value="KOKOM" description="comparative conjunction"
			example="als, wie" />
		<feat xlink:href="#KOUI" value="KOUI"
			description="subordinating conjunction with zu-infinitive" example="um [zu leben], anstatt [zu fragen]" />
		<feat xlink:href="#KOUS" value="KOUS"
			description="subordinating conjunction with sentence" example="weil, daß, damit, wenn, ob" />
		<feat xlink:href="#APPR" value="APPR" description="preposition"
			example="in [der Stadt], ohne [mich], von [jetzt an]" />
		<feat xlink:href="#APPRART" value="APPRART" description="preposition + article"
			example="im [Haus], zur [Sache]" />
		<feat xlink:href="#APPO" value="APPO" description="postposition"
			example="[ihm] zufolge, [der Sache] wegen" />
		<feat xlink:href="#APZR" value="APZR" description="circumposition, right part"
			example="[von jetzt] an" />
		<feat xlink:href="#PTKZU" value="PTKZU" description="'zu' before infinitive"
			example="zu [gehen]" />
		<feat xlink:href="#PTKNEG" value="PTKNEG" description="negating particle"
			example="nicht" />
		<feat xlink:href="#PTKVZ" value="PTKVZ" description="separated verb particle"
			example="er kommt] an, [er faehrt] rad" />
		<feat xlink:href="#PTKANT" value="PTKANT" description="answer particle"
			example="ja, nein, danke, bitte" />
		<feat xlink:href="#PTKA" value="PTKA"
			description="particle with adjektive or adverb" example="am [schoensten], zu [schnell]" />
		<feat xlink:href="#DOLLAR_COMMA" value="$," description="comma"
			example="," />
		<feat xlink:href="#DOLLAR_PERIOD" value="$." description="final punctuation"
			example=". ? ! ; :" />
		<feat xlink:href="#DOLLAR_OPENPAREN" value="$("
			description="other punctuation marks" example="- [,]()" />
		<feat xlink:href="#FM" value="FM" description="foreign material"
			example="[Er hat das mit ``] A big fish ['' uebersetzt]" />
		<feat xlink:href="#XY" value="XY" description="nonword, with special characters"
			example="3:7, H2O, D2XW3" />
		<feat xlink:href="#TRUNC" value="TRUNC" description="truncated element"
			example="An- [und Abreise]" />
		<feat xlink:href="#TWO_HYPHENS" value="--" description="untagged" />
		<feat xlink:href="#UNKNOWN" value="UNKNOWN" description="tagging of the token unknown" />

	</featList>

</paula>
