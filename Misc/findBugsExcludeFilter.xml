<?xml version="1.0" encoding="UTF-8"?>

<!-- False positive Filter for FindBugs -->

<FindBugsFilter>
  <!-- automatically generated code that we can't influence -->
  <Match>
    <Or>
      <Class name="annis.ql.parser.Parser"/>
      <Class name="de.deutschdiachrondigital.dddquery.parser.Parser"/>
      <Class name="annis.ql.lexer.Lexer"/>
      <Class name="de.deutschdiachrondigital.dddquery.lexer.Lexer"/>
    </Or>
  </Match>
  
  <!-- various System.out and System.err null pointer errors -->
  <Match>
    <Class name="annis.AnnisBaseRunner" />
    <Bug pattern="NP_ALWAYS_NULL" />
    <Or>
       <Method name="run" />
       <Method name="runInteractive" />
       <Method name="error" />
       <Method name="runCommand" />
       <Method name="checkForAnnisHome" />
    </Or>
  </Match>
  <Match>
    <Class name="annis.AnnisRunner" />
    <Bug pattern="NP_ALWAYS_NULL" />
    <Or>
       <Method name="doLanguage" />
       <Method name="doDebug" />
       <Method name="doAnnotations" />
       <Method name="doQuit" />
       <Method name="doCompareParser" />
    </Or>
  </Match>
  <Match>
    <Class name="annis.administration.AnnisAdminRunner" />
    <Bug pattern="NP_ALWAYS_NULL" />
    <Or>
       <Method name="doList" />
       <Method name="doIndexes" />
       <Method name="doAnnotations" />
       <Method name="usage" />
       <Method name="printTable" />
    </Or>
  </Match>
  <Match>
    <Class name="annis.benchmarking.BenchmarkRunner" />
    <Bug pattern="NP_ALWAYS_NULL" />
    <Or>
       <Method name="printLine" />
    </Or>
  </Match>
  <Match>
    <Class name="annis.service.internal.AnnisServiceRunner" />
    <Bug pattern="NP_ALWAYS_NULL" />
    <Or>
       <Method name="startAndDetach" />
       <Method name="daemonize" />
    </Or>
  </Match>
</FindBugsFilter>
